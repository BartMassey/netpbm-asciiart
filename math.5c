import String;

string reverse(string s) {
  string r = "";
  for (int i = length(s) - 1; i >= 0; --i)
    r += new(s[i]);
  return r;
}

void print_scale(int n, real lo, real hi) {
  real delta = (hi - lo) / (n - 2);
  printf("0 %.5f\n", 0);
  int i = 1;
  for (real v = lo; v <= hi; v += delta)
    printf("%d %.5f\n", i++, v);
}
